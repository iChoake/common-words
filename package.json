{
  "name": "www-common-words",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^16.0.0",
    "bundle-collapser": "^1.2.0",
    "compute-mean": "^3.0.0",
    "compute-median": "^2.0.0",
    "compute-mode": "^1.0.0",
    "cssnano": "^3.10.0",
    "debounce": "^1.0.0",
    "esmangle": "^1.0.0",
    "global": "^4.0.0",
    "nlcst-normalize": "^2.1.0",
    "postcss-cli": "^5.0.0",
    "rehype-cli": "^5.0.0",
    "rehype-preset-minify": "^2.0.0",
    "rehype-prevent-favicon-request": "^1.0.0",
    "retext-english": "^3.0.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "subtlex-word-frequencies": "^1.0.0",
    "unified": "^6.1.0",
    "unist-util-visit": "^1.1.0",
    "virtual-dom": "^2.1.0",
    "xo": "^0.20.0",
    "xtend": "^4.0.0"
  },
  "scripts": {
    "build:generate": "node build",
    "build:js": "browserify -t brfs -p bundle-collapser/plugin src/index.js | esmangle > dest/index.js",
    "build:css": "postcss src/index.css > dest/index.css",
    "build:html": "rehype -u preset-minify -u prevent-favicon-request src -o dest",
    "build": "npm run build:generate && npm run build:js && npm run build:css && npm run build:html",
    "lint": "xo && stylelint src/index.css",
    "test": "npm run lint && npm run build"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "no-var": "off",
      "object-shorthand": "off",
      "prefer-arrow-callback": "off"
    }
  },
  "postcss": {
    "plugins": {
      "cssnano": {
        "preset": "default"
      }
    }
  }
}
